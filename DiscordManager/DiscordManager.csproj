<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageVersion>0.0.2</PackageVersion>
        <PackageProjectUrl>https://github.com/Team-IF/DiscordManager</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/Team-IF/DiscordManager/blob/master/LICENSE</PackageLicenseUrl>
        <PackageTags>Discord</PackageTags>
        <Title>DiscordManager</Title>
        <Authors>nature@teamif.io</Authors>
        <Configurations>Debug;Release;PublishtoNuget</Configurations>
        <Platforms>AnyCPU</Platforms>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Discord.Net" Version="2.2.0" />
      <PackageReference Include="Discord.Net.Core" Version="2.2.0" />
    </ItemGroup>

</Project>
